.tab-list {
  display: flex;
  justify-content: space-around;
  align-items: center;
  position: fixed;
  bottom: 0;
  left: 0;
  z-index: 4;
  width: 100%;
  height: 102rpx;
  font-size: 32rpx;
  color: #fff;
  background-color: #fff;
  
} 
.tab-item {
  display: flex;
  align-items: center;
  position: relative;
  bottom: 0;
  height: 80rpx;
  font-weight: bold;
  color: #1C202D;
}

.tab-img {
  margin-right: 20rpx;
  width: 48rpx;
  height: 48rpx;
}
.tab-xunban {
  display: flex;
  justify-content: center;
  align-items: center;
  position: relative;
  bottom: 12rpx;
  width: 104rpx;
  height: 104rpx;
  background-color: #07C49C;
  border: 8rpx solid #fff;
  border-radius: 50%;
}
.tab-xunban .tab-img {
  margin-right: 0;
}
.tab-active {
  position: absolute;
  bottom: 24rpx;
  left: 24rpx;
  width: 24rpx;
  height: 24rpx;
  background-color: #07C49C;
  border-radius: 50%;
}